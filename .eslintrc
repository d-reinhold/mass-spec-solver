{
  "env": {
    "es6": true,
    "browser": true,
    "phantomjs": true,
    "node": true,
    "jasmine": true
  },

  "ecmaFeatures": {
    "modules": true
  },

  "globals": {
    "STATIC_CONFIG": true,
    "$": true,
    "AppsManager": true,
    "Deferred": true,
    "Dispatcher": true,
    "Factory": true,
    "jQuery": true,
    "MockPromises": true,
    "MockRouter": true,
    "React": true,
    "ReactDOM": true,
    "root": true,
    "renderWithContext": true,
    "renderWithControlledProps": true,
    "rerenderWithMergedProps": true,
    "byGuid": true,
    "describeWithWebdriver": true,
    "waitForExist": true,
    "waitForText": true,
    "login": true,
    "visitPage": true,
    "getPage": true,
    "spyOnRender": true,
    "spyOnRenderAndRenderChildren": true,
    "propsOnLastRender": true,
    "Cursor": true,
    "spyOnAsync": true,
    "_": true
  },

  "parser": "babel-eslint",

  "plugins": [
    "react"
  ],

  "rules": {
    "block-scoped-var": 2,
    "camelcase": 0,
    "complexity": 2,
    "consistent-return": 2,
    "curly": 0,
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "eol-last": 0,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 0,
    "no-empty-label": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-path-concat": 0,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 0,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "quotes": [2, "single"],
    "radix": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef": 2,
    "semi": 2,
    "jsx-quotes": [2, "prefer-double"],
    "react/no-did-mount-set-state": 0,
    "react/no-did-update-set-state": 2,
    "react/prop-types": [2, {ignore: ['children', 'className', 'id', 'style']}],
    "react/self-closing-comp": 2,
    "react/wrap-multilines": 2,
    "strict": 0,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "yoda": 2
  }
}